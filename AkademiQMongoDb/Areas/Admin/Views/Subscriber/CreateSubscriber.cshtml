@model CreateSubscriberDto
@{
    ViewData["Title"] = "CreateSubscriber";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Yeni Abonelik Oluşturma</h1>
<br />
<br />

<form method="post">
    <label>Müşteri Adı Soyadı</label>
    <input type="text" asp-for="Email" class="form-control" placeholder="Örn: google@gmail" required />
    <span asp-validation-for="Email" class="text-danger"></span>
    <br>

    <button class="btn btn-dark">Aboneliği Kaydet</button>
</form>