@{
	Layout = null;
}


<!DOCTYPE html>
<html lang="tr">

<head>
	<!-- ========== Meta Tags ========== -->
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="Chick - Chick & Restaurant HTML Template ">

	<!-- ========== Page Title ========== -->
	<title>Chick - Chick & Restaurant</title>

	<!-- ========== Favicon Icon ========== -->
	<link rel="shortcut icon" href="/image/premiumLogo.jpg" type="image/x-icon">

	<!-- ========== Start Stylesheet ========== -->
	<link href="~/css/site.css" rel="stylesheet">
	<link href="~/source/assets/css/bootstrap.min.css" rel="stylesheet">
	<link href="~/source/assets/css/font-awesome.min.css" rel="stylesheet">
	<link href="~/source/assets/css/flaticon-set.css" rel="stylesheet">
	<link href="~/source/assets/css/magnific-popup.css" rel="stylesheet">
	<link href="~/source/assets/css/swiper-bundle.min.css" rel="stylesheet">
	<link href="~/source/assets/css/animate.min.css" rel="stylesheet">
	<link href="~/source/assets/css/bootstrap-datepicker3.css" rel="stylesheet">
	<link href="~/source/assets/css/validnavs.css" rel="stylesheet">
	<link href="~/source/assets/css/helper.css" rel="stylesheet">
	<link href="~/source/assets/css/unit-test.css" rel="stylesheet">
	<link href="~/source/assets/css/shop.css" rel="stylesheet">
	<link href="~/source/assets/css/style.css" rel="stylesheet">
	<link href="style.css" rel="stylesheet">
	<!-- ========== End Stylesheet ========== -->
	<!--[if lte IE 9]>
		<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
	<![endif]-->

</head>

<body>

	<div class="radio-btn-light">
		<div class="radio-inner-light"></div>
	</div>

	<!-- Start Preloader
	============================================= -->
	<div id="preloader">
		<div id="restan-preloader" class="restan-preloader">
			<div class="animation-preloader">
				<div class="spinner"></div>
			</div>
			<div class="loader">
				<div class="row">
					<div class="col-3 loader-section section-left">
						<div class="bg"></div>
					</div>
					<div class="col-3 loader-section section-left">
						<div class="bg"></div>
					</div>
					<div class="col-3 loader-section section-right">
						<div class="bg"></div>
					</div>
					<div class="col-3 loader-section section-right">
						<div class="bg"></div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- End Preloader -->
	<!-- Header
	============================================= -->
	<header>
		<!-- Start Navigation -->
		<nav class="navbar mobile-sidenav navbar-sticky navbar-default validnavs navbar-fixed white no-background nav-border">

			<!-- Start Top Search -->
			<div class="top-search">
				<div class="container-xl">
					<div class="input-group">
						<span class="input-group-addon"><i class="fa fa-search"></i></span>
						<input type="text" class="form-control" placeholder="Search">
						<span class="input-group-addon close-search"><i class="fa fa-times"></i></span>
					</div>
				</div>
			</div>
			<!-- End Top Search -->


			<div class="container d-flex justify-content-between align-items-center">

				<!-- Start Header Navigation -->
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-menu">
						<i class="fa fa-bars"></i>
					</button>
					<a class="navbar-brand" href="index.html">
						<img src="~/image/logo_Yazili.jpg" class="logo logo-display" alt="Logo">
						<img src="~/image/logo_Yazili.jpg" class="logo logo-scrolled" alt="Logo">
					</a>
				</div>
				<!-- End Header Navigation -->
				<!-- Collect the nav links, forms, and other content for toggling -->
				<div class="collapse navbar-collapse" id="navbar-menu">

					<img src="~/source/assets/img/logo.png" alt="Logo">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-menu">
						<i class="fa fa-times"></i>
					</button>

					<ul class="nav navbar-nav navbar-right" data-in="fadeInDown" data-out="fadeOutUp">
						<li>
							<a href="/Default/Index/" class="active">Ana Sayfa</a>
						</li>
						<li>
							<a href="/About/Index/">Hakkımızda</a>
						</li>
						<li>
							<a href="/Menu/Index/" class="active">Menü</a>
						</li>
						<li>
							<a href="/Booking/Index/">Rezervasyon</a>
						</li>
						<li>
							<a href="/Contact/Index/">İletişim</a>
						</li>




					</ul>
				</div><!-- /.navbar-collapse -->
				<div class="attr-right">
					<!-- Start Atribute Navigation -->
					<div class="attr-nav">
						<ul>
							<li class="button">
								<a href="/Register/Signup/">Kayıt Ol</a>
							</li>
						</ul>
					</div>
					<!-- End Atribute Navigation -->


				</div>
				<!-- Overlay screen for menu -->
				<div class="overlay-screen"></div>
				<!-- End Overlay screen for menu -->

		</nav>
		<!-- End Navigation -->
	</header>
	<!-- End Header -->
	@RenderBody()
	<!-- Start Footer
	============================================= -->
	<footer class="bg-dark footer-style-one text-light">
		<div class="container">
			<div class="row">
				<!-- Singel Item -->
				<div class="col-lg-3 col-md-6 footer-item mt-50">
					<div class="f-item about">
						<img src="~/image/logo_Yazili.jpg" alt="_footerLogo">
						<p>
							Yemek cennetimizde enfes tarifler ve ilham kaynakları keşfedin.
						</p>
						<ul class="opening-list">
							<li>
								Pazartesi  -  Cuma <span class="text-end">8:00 AM  -  6:00 PM</span>
							</li>
							<li>
								Cumartesi <span class="text-end">9:00 AM  -  5:00 PM</span>
							</li>
						</ul>
					</div>
				</div>
				<!-- End Singel Item -->
				<!-- Singel Item -->
				<div class="col-lg-2 col-md-6 mt-50 footer-item pl-50 pl-md-15 pl-xs-15">
					<div class="f-item link">
						<h4 class="widget-title">Keşfet</h4>
						<ul>
							<li>
								<a href="/Default/Index/">Ana Sayfa</a>
							</li>
							<li>
								<a href="/About/Index/">Hakkımızda</a>
							</li>
							<li>
								<a href="/Contact/Index/">İletişim</a>
							</li>
							<li>
								<a href="/Menu/Index/">Menü</a>
							</li>
							<li>
								<a href="/Booking/Index/">Rezervasyon</a>
							</li>
						</ul>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 footer-item  mt-50">
					@await Component.InvokeAsync("_FooterContactPartial")
				</div>

				<div class="col-lg-4 col-md-6 footer-item mt-50">
					<div class="f-item newsletter">
						<h4 class="widget-title">Bülten</h4>
						<p>
							Size özel indirimleri ve haberleri kaçırmamak için mail bültenimize abone olmayın unutmayın.
						</p>
						<form asp-controller="UILayout" asp-action="Subscribe" method="post">
							<input type="email" placeholder="E-posta Adresiniz" class="form-control" name="Email" required>
							<button type="submit"> Abone Ol <i class="fas fa-long-arrow-right"></i></button>
						</form>

					</div>
				</div>


			</div>

			<div class="footer-bottom">
				<div class="row align-center">
					<div class="col-lg-6">
						<p>
							© Copyright 2025 Foodu. All Rights Reserved
						</p>
					</div>
				</div>
			</div>
			<!-- End Footer Bottom -->
		</div>
	</footer>

	<div id="notification-data"
		 data-error="@TempData["SubscribeMessage"]"
		 data-success="@TempData["SubscriberMessage"]"
		 style="display: none;">
	</div>
	<!-- End Footer -->
	<!-- jQuery Frameworks
	============================================= -->
	<script src="~/source/assets/js/jquery-3.6.0.min.js"></script>
	<script src="~/source/assets/js/bootstrap.bundle.min.js"></script>
	<script src="~/source/assets/js/jquery.appear.js"></script>
	<script src="~/source/assets/js/swiper-bundle.min.js"></script>
	<script src="~/source/assets/js/progress-bar.min.js"></script>
	<script src="~/source/assets/js/wow.min.js"></script>
	<script src="~/source/assets/js/isotope.pkgd.min.js"></script>
	<script src="~/source/assets/js/imagesloaded.pkgd.min.js"></script>
	<script src="~/source/assets/js/magnific-popup.min.js"></script>
	<script src="~/source/assets/js/count-to.js"></script>
	<script src="~/source/assets/js/jquery.nice-select.min.js"></script>
	<script src="~/source/assets/js/YTPlayer.min.js"></script>
	<script src="~/source/assets/js/loopcounter.js"></script>
	<script src="~/source/assets/js/validnavs.js"></script>
	<script src="~/source/assets/js/bootstrap-datepicker.js"></script>
	<script src="~/source/assets/js/gsap.js"></script>
	<script src="~/source/assets/js/ScrollTrigger.min.js"></script>
	<script src="~/source/assets/js/SplitText.min.js"></script>
	<script src="~/source/assets/js/main.js"></script>
	<script src="~/js/site.js"></script>

	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>



</body>
</html>